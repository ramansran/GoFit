<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"

    android:layout_height="match_parent"
    android:background="@color/White"
    tools:context=".Profile_Frag">


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline50"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.1" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline51"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.2" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline52"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.3" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline53"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.4" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline54"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.5" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline55"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.6" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline56"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.7"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline57"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.8" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline58"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.9" />

    <RelativeLayout
        android:id="@+id/p_upper_box"
        android:layout_width="match_parent"
        android:layout_height="130dp"
        android:layout_marginBottom="@dimen/_30sdp"
        android:background="@drawable/third_newback"
        app:layout_constraintBottom_toTopOf="@+id/guideline51"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="20dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="24dp"
            android:layout_marginBottom="14dp"
            android:src="@drawable/viduyat" />

        <TextView
            android:id="@+id/User_Name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_marginStart="12dp"
            android:layout_marginLeft="12dp"
            android:layout_marginTop="50dp"
            android:layout_toEndOf="@+id/imageView4"
            android:layout_toRightOf="@+id/imageView4"
            android:fontFamily="sa"
            android:text="Viduyat"
            android:textColor="@color/fill1"
            android:textSize="14dp" />

        <TextView
            android:id="@+id/User_Email"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/User_Name"
            android:layout_marginStart="12dp"
            android:layout_marginLeft="12dp"
            android:layout_marginTop="4dp"
            android:layout_toEndOf="@+id/imageView4"
            android:layout_toRightOf="@+id/imageView4"
            android:fontFamily="sans-serif"
            android:text="0002sran@gmail.com"
            android:textColor="@color/White"
            android:textSize="10dp" />

        <TextView
            android:id="@+id/User_type_textview"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/User_Email"
            android:layout_marginStart="12dp"
            android:layout_marginLeft="12dp"
            android:layout_marginTop="4dp"
            android:layout_toEndOf="@+id/imageView4"
            android:layout_toRightOf="@+id/imageView4"
            android:text="Normal User"
            android:textColor="@color/fill1"
            android:textSize="10dp" />
    </RelativeLayout>

    <LinearLayout
        android:id="@+id/Completed_task_layout"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:orientation="horizontal"
        android:background="@drawable/new_baackground3"
        app:layout_constraintBottom_toTopOf="@+id/guideline52"
        app:layout_constraintTop_toBottomOf="@+id/guideline51"
        >

        <ImageView
            android:layout_weight="1"
            android:layout_width="0dp"
            android:layout_height="30dp"
            android:layout_gravity="center"
            android:scaleType="fitCenter"
            android:src="@drawable/note2">
        </ImageView>

        <TextView
            android:layout_weight="2"
            android:layout_width="0dp"
            android:layout_gravity="center"
            android:text="Completed Tasks"
            android:textStyle="bold"
            android:fontFamily="sans-serif"
            android:layout_height="30dp">
        </TextView>

        <TextView
            android:layout_weight="1"
            android:text="3"
            android:textAlignment="center"
            android:textStyle="bold"
            android:layout_gravity="center"
            android:layout_width="0dp"
            android:layout_height="30dp">

        </TextView>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:orientation="horizontal"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:background="@drawable/new_baackground3"
        app:layout_constraintBottom_toTopOf="@+id/guideline53"
        app:layout_constraintTop_toBottomOf="@+id/guideline52">

        <ImageView
            android:layout_weight="1"
            android:layout_width="0dp"
            android:layout_height="30dp"
            android:layout_gravity="center"
            android:scaleType="fitCenter"
            android:src="@drawable/king2">
        </ImageView>

        <TextView
            android:layout_weight="2"
            android:layout_width="0dp"
            android:layout_gravity="center"
            android:text="Become Trainer"
            android:textStyle="bold"
            android:fontFamily="sans-serif"
            android:layout_height="30dp">
        </TextView>

        <TextView
            android:layout_weight="1"
            android:text=""
            android:textAlignment="center"
            android:textStyle="bold"
            android:layout_gravity="center"
            android:layout_width="0dp"
            android:layout_height="30dp">

        </TextView>

    </LinearLayout>



    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:orientation="horizontal"
        android:background="@drawable/new_baackground3"
        android:layout_marginRight="10dp"
        android:layout_marginLeft="10dp"
        app:layout_constraintBottom_toTopOf="@+id/guideline54"
        app:layout_constraintTop_toBottomOf="@+id/guideline53"
        >


        <ImageView
            android:layout_weight="1"
            android:layout_width="0dp"
            android:layout_height="30dp"
            android:layout_gravity="center"
            android:scaleType="fitCenter"
            android:src="@drawable/shared">
        </ImageView>

        <TextView
            android:layout_weight="2"
            android:layout_width="0dp"
            android:layout_gravity="center"
            android:text="Shared Posts"
            android:textStyle="bold"
            android:fontFamily="sans-serif"
            android:layout_height="30dp">
        </TextView>

        <TextView
            android:layout_weight="1"
            android:text="5"
            android:textAlignment="center"
            android:textStyle="bold"
            android:layout_gravity="center"
            android:layout_width="0dp"
            android:layout_height="30dp">

        </TextView>


    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:orientation="horizontal"
        android:layout_marginRight="10dp"
        android:layout_marginLeft="10dp"
        android:background="@drawable/new_baackground3"
        app:layout_constraintBottom_toTopOf="@+id/guideline55"
        app:layout_constraintTop_toBottomOf="@+id/guideline54"
        >

        <ImageView
            android:layout_weight="1"
            android:layout_width="0dp"
            android:layout_height="30dp"
            android:layout_gravity="center"
            android:scaleType="fitCenter"
            android:src="@drawable/clock">
        </ImageView>

        <TextView
            android:layout_weight="2"
            android:layout_width="0dp"
            android:layout_gravity="center"
            android:text="Target History"
            android:textStyle="bold"
            android:fontFamily="sans-serif"
            android:layout_height="30dp">
        </TextView>

        <TextView
            android:layout_weight="1"
            android:text=""
            android:textAlignment="center"
            android:textStyle="bold"
            android:layout_gravity="center"
            android:layout_width="0dp"
            android:layout_height="30dp">

        </TextView>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:orientation="horizontal"
        android:background="@drawable/new_baackground3"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        app:layout_constraintBottom_toTopOf="@+id/guideline56"
        app:layout_constraintTop_toBottomOf="@+id/guideline55">

        <ImageView
            android:layout_weight="1"
            android:layout_width="0dp"
            android:layout_height="30dp"
            android:layout_gravity="center"
            android:scaleType="fitCenter"
            android:src="@drawable/edit">
        </ImageView>

        <TextView
            android:layout_weight="2"
            android:layout_width="0dp"
            android:layout_gravity="center"
            android:text="Edit Profile"
            android:textStyle="bold"
            android:fontFamily="sans-serif"
            android:layout_height="30dp">
        </TextView>

        <TextView
            android:layout_weight="1"
            android:text=""
            android:textAlignment="center"
            android:textStyle="bold"
            android:layout_gravity="center"
            android:layout_width="0dp"
            android:layout_height="30dp">

        </TextView>


    </LinearLayout>





    <LinearLayout
        android:id="@+id/bottom_menu"
        android:layout_width="match_parent"
        android:background="@color/transparent"
        android:layout_height="@dimen/_35sdp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintVertical_bias="0.93"
        app:layout_constraintTop_toTopOf="@+id/guideline76">



        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_weight="1"
            android:layout_gravity="bottom"
            android:src="@drawable/exercise"
            android:id="@+id/exercise_screen">
        </ImageView>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_weight="1"
            android:layout_gravity="bottom"
            android:src="@drawable/trainer"
            android:id="@+id/trainers_screen">
        </ImageView>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_weight="1"
            android:src="@drawable/alarm"
            android:layout_gravity="bottom"
            android:id="@+id/Remainders_screen">
        </ImageView>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_gravity="bottom"
            android:layout_weight="1"
            android:src="@drawable/chat"

            android:id="@+id/Chat_screen">
        </ImageView>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_gravity="bottom"
            android:layout_weight="1"
            android:src="@drawable/profile"
            android:id="@+id/Profile_screen">
        </ImageView>
    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>