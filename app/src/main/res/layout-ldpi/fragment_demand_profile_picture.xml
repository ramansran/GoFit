<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DemandProfilePictureFrag">


    <RelativeLayout
        android:layout_width="429dp"
        android:layout_height="267dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginEnd="8dp"
        android:background="@color/askusertypestatusbar"
        android:layout_marginRight="8dp"
        app:layout_constraintBottom_toTopOf="@+id/frameLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.089">

        <RelativeLayout
            android:id="@+id/frameLayout"
            android:layout_width="310dp"
            android:layout_height="267dp"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginBottom="304dp"
            android:background="@drawable/profile_widget">

            <ImageView
                android:id="@+id/Demand_Profile_ImageView"
                android:layout_width="64dp"
                android:layout_height="57dp"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_marginTop="100dp"
                android:layout_marginEnd="229dp"
                android:layout_marginRight="229dp"
                android:scaleType="fitXY"/>

            <Button
                android:id="@+id/button2"
                android:layout_width="78dp"
                android:layout_height="81dp"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_marginTop="94dp"
                android:layout_marginEnd="50dp"
                android:layout_marginRight="50dp"
                android:background="@drawable/upload_button" />
        </RelativeLayout>
    </RelativeLayout>


    <TextView
        android:id="@+id/textView3"
        android:layout_width="84dp"
        android:layout_height="25dp"
        android:layout_marginStart="60dp"
        android:layout_marginLeft="60dp"
        android:layout_marginBottom="8dp"
        android:text="Skip "
        android:textColor="@color/fill1"
        android:textSize="15dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frameLayout"
        app:layout_constraintVertical_bias="0.09" />

    <Button
        android:id="@+id/button3"
        android:layout_width="84dp"
        android:layout_height="23dp"
        android:layout_marginTop="7dp"
        android:layout_marginEnd="68dp"
        android:layout_marginRight="68dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/edit_text_style"
        android:text="Next"
        android:textColor="@color/White"
        android:textSize="11dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frameLayout"
        app:layout_constraintVertical_bias="0.066" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="317dp"
        android:layout_height="200dp"
        android:src="@drawable/logintopimage"
        tools:layout_editor_absoluteX="-56dp"
        tools:layout_editor_absoluteY="155dp" />

</androidx.constraintlayout.widget.ConstraintLayout>